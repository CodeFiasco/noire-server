{
  "name": "noire-server",
  "version": "0.4.0",
  "description": "Noire Server",
  "main": "lib/index.js",
  "keywords": [
    "hapi",
    "boilerplate"
  ],
  "scripts": {
    "start": "node lib/index.js",
    "test": "lab",
    "codecov": "mkdir .coverage 2>/dev/null; lab -r lcov > .coverage/lcov.info && codecov",
    "reset": "knex migrate:latest; knex seed:run",
    "secret": "node lib/utils/auth.js secret",
    "token": "node lib/utils/auth.js token",
    "semantic": "gulp --gulpfile client/semantic/gulpfile.js build",
    "webpack": "webpack --hide-modules true --config webpack.config.js",
    "webpack:watch": "webpack --config webpack.config.js --watch",
    "build": "npm run clean; npm run semantic; npm run webpack",
    "clean": "rm -rf client/dist/; rm -rf client/semantic/dist"
  },
  "repository": {
    "type": "git",
    "url": "github:academia-de-codigo/noire-server"
  },
  "author": "Academia de CÃ³digo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/academia-de-codigo/noire-server/issues"
  },
  "homepage": "https://github.com/academia-de-codigo/noire-server",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "boom": "^7.1.1",
    "chalk": "^2.3.0",
    "crumb": "^7.0.0",
    "exiting": "^4.0.0",
    "glue": "^5.0.0",
    "handlebars": "^4.0.11",
    "hapi": "^17.2.0",
    "hapi-auth-jwt2": "github:salzhrani/hapi-auth-jwt2#v-17",
    "hapi-pagination": "^2.0.0",
    "hapi-pino": "^4.0.3",
    "hoek": "^5.0.2",
    "inert": "^5.1.0",
    "joi": "^13.1.1",
    "jsonwebtoken": "^8.1.1",
    "knex": "^0.14.2",
    "lout": "github:henrikdahl/lout#hapi-v17",
    "objection": "^0.9.4",
    "pg": "^7.4.1",
    "rotating-file-stream": "^1.3.4",
    "sqlite3": "^3.1.13",
    "vision": "^5.3.1"
  },
  "devDependencies": {
    "code": "^5.1.2",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.9",
    "expose-loader": "^0.7.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "form-serializer": "^2.5.0",
    "glob": "^7.1.2",
    "gulp": "^3.9.1",
    "imports-loader": "^0.7.1",
    "jquery": "^3.3.1",
    "lab": "^15.2.1",
    "layout-webpack-plugin": "^0.0.6",
    "semantic-ui": "^2.2.14",
    "sinon": "^4.2.2",
    "mock-require": "^3.0.1",
    "style-loader": "^0.20.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0"
  }
