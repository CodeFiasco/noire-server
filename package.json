{
    "name": "noire-server",
    "version": "0.4.0",
    "description": "Noire Server",
    "main": "lib/index.js",
    "keywords": [
        "hapi",
        "boilerplate"
    ],
    "scripts": {
        "start": "node lib/index.js",
        "test": "lab",
        "codecov": "mkdir .coverage 2>/dev/null; lab -r lcov > .coverage/lcov.info && codecov",
        "reset": "knex migrate:latest; knex seed:run",
        "secret": "node lib/utils/auth.js secret",
        "token": "node lib/utils/auth.js token",
        "semantic": "gulp --gulpfile client/semantic/gulpfile.js build",
        "webpack": "webpack --hide-modules true --config webpack/webpack.dev.js",
        "webpack:watch": "webpack --config webpack.config.js --watch",
        "build": "npm run clean; npm run semantic; npm run webpack",
        "clean": "rm -rf client/dist/; rm -rf client/semantic/dist"
    },
    "repository": {
        "type": "git",
        "url": "github:academia-de-codigo/noire-server"
    },
    "author": "Academia de CÃ³digo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/academia-de-codigo/noire-server/issues"
    },
    "homepage": "https://github.com/academia-de-codigo/noire-server",
    "dependencies": {
        "bcrypt": "^1.0.3",
        "boom": "^7.2.0",
        "chalk": "^2.3.2",
        "crumb": "^7.0.0",
        "exiting": "^4.0.0",
        "glue": "^5.0.0",
        "handlebars": "^4.0.11",
        "hapi": "^17.2.3",
        "hapi-auth-jwt2": "^8.0.1",
        "hapi-pagination": "^2.0.0",
        "hapi-pino": "^4.0.3",
        "hoek": "^5.0.3",
        "inert": "^5.1.0",
        "joi": "^13.1.2",
        "jsonwebtoken": "^8.2.0",
        "knex": "0.14.2",
        "lout": "^11.0.0",
        "objection": "^1.0.0",
        "pg": "^7.4.1",
        "sqlite3": "^3.1.13",
        "vision": "^5.3.2"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^0.1.19",
        "code": "^5.2.0",
        "codecov": "^3.0.0",
        "copy-webpack-plugin": "^4.5.1",
        "css-loader": "^0.28.10",
        "eslint-config-prettier": "^2.9.0",
        "expose-loader": "^0.7.5",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "form-serializer": "^2.5.0",
        "glob": "^7.1.2",
        "gulp": "^3.9.1",
        "imports-loader": "^0.8.0",
        "jquery": "^3.3.1",
        "lab": "^15.4.1",
        "layout-webpack-plugin": "^0.0.6",
        "mock-require": "^3.0.1",
        "prettier": "^1.11.1",
        "prettier-eslint": "^8.8.1",
        "semantic-ui": "^2.3.1",
        "sinon": "^4.4.8",
        "style-loader": "^0.20.3",
        "url-loader": "^1.0.1",
        "webpack": "3.11.0"
    }
}
